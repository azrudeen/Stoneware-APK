<?xml version="1.0" encoding="utf-8"?>

<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#D9D9D9"
    android:foregroundTint="#00CC1C1C"
    tools:context=".HomePage">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <View
            android:id="@+id/navigation_bar"
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="#F34B4B"/>

        <ImageButton
            android:id="@+id/menu_icon"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:layout_marginStart="-411dp"
            android:layout_marginTop="0dp"
            android:background="#00FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/menu" />


        <ImageButton
            android:id="@+id/Home_icon"
            android:layout_width="150dp"
            android:layout_height="match_parent"
            android:background="#00FFFFFF"
            android:scaleType="fitCenter"
            app:srcCompat="@drawable/home_icon" />


        <ImageButton
            android:id="@+id/search_bar"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:padding="5dp"
            android:layout_marginStart="100dp"
            android:background="#00FFFFFF"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/search_4"/>


        <ImageButton
            android:id="@+id/Account_icon"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:padding="5dp"
            android:layout_marginStart="5dp"
            android:background="#00FFFFFF"
            android:scaleType="fitXY"
            app:srcCompat="@drawable/account_icon"/>
    </LinearLayout>


    <!-- GridLayout  -->

    <ImageButton
        android:id="@+id/home_image_button"
        android:layout_width="350dp"
        android:layout_height="200dp"
        android:gravity="center_horizontal"
        android:layout_marginTop="80dp"
        android:background="@drawable/rounded_corners"
        android:layout_gravity="center_horizontal"
        android:src="@drawable/pngtree_marble_natural_breccia"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp">

        <Button
            android:id="@+id/Popular"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginStart="30dp"
            android:background="@drawable/button_background"
            android:text="Popular"
            android:textColor="@color/white"/>

        <Button
            android:id="@+id/Filter"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginStart="50dp"
            android:background="@drawable/button_background"
            android:text="@string/filter_button_text"
            android:textAlignment="viewStart"
            android:paddingStart="10dp"
            android:textColor="@color/white"/>
    </LinearLayout>


