<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D9D9D9"
    android:foregroundTint="#00CC1C1C"
    android:orientation="vertical"
    tools:context=".HomePage">


        <include layout="@layout/toolbar_layout" />


    <ImageButton

        android:id="@+id/home_image_button"
        android:layout_width="350dp"
        android:layout_height="175dp"
        android:layout_below="@+id/toolbar_layout"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:background="@drawable/image_rounded_corners"
        android:contentDescription="@string/image_button"
        android:gravity="center_horizontal"
        android:scaleType="fitXY"
        android:padding="4dp"
        android:src="@drawable/image_button_1"
        app:shapeAppearanceOverlay="@style/roundImageView"/>


    <LinearLayout
        android:id="@+id/buttons"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/home_image_button"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="30dp"
        android:gravity="center"
        android:orientation="horizontal">

        <Button
            android:id="@+id/Popular"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:background="@drawable/button_background"
            android:text="Popular"
            android:textColor="@color/white" />

        <Button
            android:id="@+id/Filter"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_marginStart="50dp"
            android:background="@drawable/button_background"
            android:paddingStart="20dp"
            android:text="@string/filter_button_text"
            android:textAlignment="viewStart"
            android:textColor="@color/white" />

    </LinearLayout>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/buttons">

        <androidx.gridlayout.widget.GridLayout
            android:id="@+id/gridLayout"
